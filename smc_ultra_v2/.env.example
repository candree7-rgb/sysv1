# SMC Ultra V2 - Environment Variables
# =====================================

# Bybit API (Required for live trading)
BYBIT_API_KEY=your_api_key_here
BYBIT_API_SECRET=your_api_secret_here
BYBIT_TESTNET=true  # Set to false for live trading

# Trading Mode
TRADING_MODE=backtest  # backtest, paper, live

# Risk Settings
RISK_LEVEL=moderate  # conservative, moderate, aggressive
MIN_CONFIDENCE=85
MAX_LEVERAGE=50

# Logging
LOG_LEVEL=INFO
